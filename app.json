{
  "expo": {
    "name": "Hike Around",
    "slug": "hikearound-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "web"
    ],
    "version": "0.0.1",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.tryhikearound",
      "config": {
        "googleMapsApiKey": "AIzaSyDzhRGewrBXqU6XPG5Bdl29JpPRPNtdilY"
      }
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "hikearound",
            "project": "hikearound",
            "authToken": "",
          }
        }
      ]
    },
    "description": "An iOS app that offers curated and personalized hikes.",
    "githubUrl": "https://github.com/pdugan20/hikearound-app"
  }
}
